{
    "manifest_version": 3,
    "name": "Scraper",
    "version": "1.1",
    "description": "Scrape linkedIn's user data.",
    "action":{
        "default_title": "Scraper"
    },
    "background":{
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "content_scripts": [{
        "matches": ["https://*/*"],
        "js": ["scripts/content.js"],
        "css": ["scripts/content.css"]     
    }],
    "host_permissions": ["<all_urls>"],
    "permissions":["storage", "contextMenus","activeTab","tabs"]
}